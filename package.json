{
  "name": "willhastings.me",
  "version": "0.0.1",
  "description": "The blog and portfolio website of Will Hastings.",
  "main": "index.js",
  "scripts": {
    "analyze": "./bin/analyze-client-build",
    "build-app": "webpack --progress --color --config=config/webpack.config.js",
    "build-content": "gulp",
    "build-with-profile": "SHOULD_PROFILE=true NODE_ENV=production npm run build-app",
    "build": "npm run build-content && npm run build-app",
    "c": "./bin/repl",
    "db:clear": "./bin/db clear",
    "db:seed": "./bin/db seed",
    "debug": "node-debug index.js",
    "dev": "concurrently -p '[{name}]' -n 'CLIENT,SERVER' -c 'bgBlue.bold,bgGreen.bold' 'npm run dev-client' 'npm run dev-server'",
    "dev-client": "webpack-dev-server --config=config/webpack/client.config.js",
    "dev-server": "npm run build-app -- --watch",
    "g": "plop",
    "migrations": "sequelize db:migrate",
    "postinstall": "npm run build && npm run migrations",
    "start": "./bin/start",
    "test": "testem"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whastings/willhastings.me.git"
  },
  "author": "Will Hastings",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/whastings/willhastings.me/issues"
  },
  "homepage": "https://github.com/whastings/willhastings.me#readme",
  "engines": {
    "node": "^6.11.1"
  },
  "dependencies": {
    "@whastings/css_utils": "^0.1.0",
    "@whastings/js_utils": "^0.5.1",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node": "^6.0.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.3.13",
    "babel-resolver": "0.0.18",
    "babel-runtime": "^6.6.1",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.2",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.0",
    "core-js": "^2.4.1",
